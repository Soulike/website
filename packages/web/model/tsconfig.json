{
  // tsc-alias is unhappy with package name extends now. Use path instead to mitigate.
  // TODO: check if fixed after tsc-alias updates.
  "extends": "./node_modules/@website/ts-config/src/tsconfig.bundler.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./dist",
    "paths": {
      "@models/*": ["./src/models/*"],
      "@react/*": ["./src/react/*"]
    }
  }
}
